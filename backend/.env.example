# ScanSan API Configuration
SCANSAN_API_KEY=your_api_key_here
SCANSAN_BASE_URL=https://api.scansan.com/v1

# Feature flags
USE_SCANSAN=false

# Model Configuration
# Options: stub | local_pickle | http
MODEL_PROVIDER=stub

# For local_pickle provider
MODEL_PATH=./models/model.pkl

# For http provider
MODEL_HTTP_URL=http://localhost:8001/predict

# Cache settings
CACHE_TTL_SECONDS=3600
ENABLE_CACHE=true
