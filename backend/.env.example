# OpenRouter LLM Configuration
OPENROUTER_API_KEY=sk-or-v1-xxx
LLM_MODEL=openai/gpt-4o-mini
LLM_BASE_URL=https://openrouter.ai/api/v1

# ScanSan API Configuration
SCANSAN_API_KEY=your_api_key_here
SCANSAN_BASE_URL=https://api.scansan.com/v1

# Feature flags
USE_SCANSAN=false

# Model Configuration (PLACEHOLDER - teammate will change)
# Options: stub | local_pickle | http
MODEL_PROVIDER=stub

# For local_pickle provider (PLACEHOLDER)
MODEL_PATH=./models/model.pkl

# For http provider (PLACEHOLDER)
MODEL_HTTP_URL=http://localhost:8001/predict

# Cache settings
CACHE_TTL_SECONDS=3600
ENABLE_CACHE=true
